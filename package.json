{
  "name": "it-learning-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:all": "concurrently -n \"Next.js,Email Worker,Suggestion Worker\" -c \"cyan,yellow,magenta\" \"npm run dev\" \"npm run worker:email\" \"npm run worker:suggestion\"",
    "dev:email": "concurrently -n \"Next.js,Email Worker\" -c \"cyan,yellow\" \"npm run dev\" \"npm run worker:email\"",
    "build": "next build",
    "start": "next start",
    "start:all": "concurrently \"npm run start\" \"npm run worker:email\" \"npm run worker:suggestion\"",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:setup": "npm run db:generate && npm run db:migrate",
    "postinstall": "prisma generate",
    "stripe:setup": "tsx scripts/setup-stripe-products.ts",
    "stripe:portal": "tsx scripts/setup-stripe-portal.ts",
    "delete-user": "tsx scripts/delete-user.ts",
    "update-role": "tsx scripts/update-user-role.ts",
    "worker:email": "tsx src/workers/start-worker.ts",
    "worker:suggestion": "tsx src/workers/start-suggestion-worker.ts",
    "seed:email-templates": "tsx scripts/seed-email-templates.ts"
  },
